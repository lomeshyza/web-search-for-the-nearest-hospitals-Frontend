{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "npm run lint --fix",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-one": "vitest --run",
    "test": "vitest",
    "format": "prettier . --check",
    "format-fix": "prettier . --write",
    "check": "npm run format-fix && npm run lint-fix && npm run build && npm run test-one",
    "commit": "npm run check && git add -A && cz"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.2",
    "@storybook/addon-interactions": "^7.6.2",
    "@storybook/addon-links": "^7.6.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.6.2",
    "@storybook/react": "^7.6.2",
    "@storybook/react-vite": "^7.6.2",
    "@storybook/test": "^7.6.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "prettier": "3.1.0",
    "sass": "^1.69.5",
    "storybook": "^7.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.6",
    "husky": "^8.0.0"
  },
  "lint-staged": {
    "*.{js,jsx}": "npm run lint",
    "*.{js,jsx,css,md,html,json}": "npm run format"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
